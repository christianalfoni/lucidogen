{
  "name": "@lucidogen/test-repo",
  "version": "0.0.0",
  "description": "Test repo for release and travis",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lucidogen/lucidogen.git"
  },
  "author": "Gaspard Bucher <gaspard@lucidogen.io>",
  "license": "MIT",
  "private": false,
  "types": "lib/index.d.ts",
  "devDependencies": {
    "@types/jest": "^22.2.3",
    "commitizen": "^2.10.1",
    "coveralls": "^3.0.1",
    "cz-customizable": "^5.2.0",
    "cz-customizable-ghooks": "^1.5.0",
    "jest": "^23.0.1",
    "prettier": "^1.13.3",
    "ts-jest": "^22.4.6",
    "tslint": "^5.10.0",
    "tslint-config-prettier": "^1.13.0",
    "tslint-react": "^3.6.0",
    "typescript": "^2.8.3"
  },
  "scripts": {
    "build": "tsc -p setup/tsconfig.build.json",
    "coverage:upload": "cat ./coverage/lcov.info | coveralls",
    "format":
      "npm run lint -- --fix && prettier --write \"src/**/*.ts\" \"src/**/*.css\" \"src/**/*.json\" \"*.json\" \"setup/**/*\"",
    "lint": "tslint -c tslint.json 'src/**/*.ts'",
    "prepare": "npm run build",
    "release": "babel-node --presets env ./setup/release/index.js",
    "test": "jest -c setup/jest.js",
    "test:watch": "jest -c setup/jest.js --watch --coverage false"
  }
}
